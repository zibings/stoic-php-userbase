{
  "name": "stoic/userbase",
  "description": "Basic user component, includes simple authentication framework",
  "version": "0.1.0",
  "homepage": "https://stoic-framework.com",
  "autoload": {
    "psr-0": {
      "Stoic\\User\\": "./User"
    },
    "psr-4": {
      "Stoic\\User\\":  "./User"
    },
    "exclude-from-classmap": [
      "./Tests/"
    ],
    "files": ["QueryBuilder.php"]
  },
  "require": {
    "php": "^7.2",
    "stoic/web": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "bin": [
    "stoic-install-users"
  ]
}
